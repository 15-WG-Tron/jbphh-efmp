{
  "name": "jbphh-efmp",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 1",
    "prepare": "husky install",
    "format-check": "prettier --check .",
    "format": "prettier --write .",
    "type-check": "tsc --pretty --noEmit",
    "preview": "npx vite preview",
    "test:unit": "vitest run",
    "test": "vitest watch --coverage",
    "test-ui": "npx vite preview --outDir html",
    "test:coverage": "vitest run --coverage && open ./coverage/index.html",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "createDevDatabase": "supabase init && supabase start && directus database install && prisma migrate dev --skip-seed && directus schema apply --yes ./snapshot.yaml && prisma db seed",
    "db:generate": "prisma generate",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:migrate": "prisma migrate dev ",
    "db:studio": "prisma studio",
    "db:status": "supabase status",
    "db:reset": "prisma migrate reset --force && npx directus schema apply --yes ./snapshot.yaml ",
    "db:seed": "prisma db seed && npx directus schema apply",
    "db:schema-format": "npx prisma format",
    "createSnapshot" : "npx directus schema snapshot --yes ./snapshot.yaml",
    "startDirectus" : "npx directus start",
  },
  "dependencies": {
    "@directus/sdk": "^11.0.3",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@jest/types": "^29.5.0",
    "@mui/icons-material": "^5.13.7",
    "@mui/material": "^5.13.7",
    "@prisma/client": "^5.3.1",
    "@refinedev/strapi-v4": "^5.2.0",
    "@tanstack/react-query": "^4.29.14",
    "@tanstack/react-query-devtools": "^4.29.14",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "18.2.9",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "axios": "^1.4.0",
    "casual": "^1.6.2",
    "directus": "^10.6.1",
    "eslint-config-next": "13.4.4",
    "next": "13.4.4",
    "pg": "^8.11.3",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "^5.1.3",
    "whatwg-fetch": "^3.6.2",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@directus/cli": "^9.14.3",
    "@tanstack/eslint-plugin-query": "^4.29.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^0.32.0",
    "@vitest/ui": "^0.32.0",
    "cypress": "^12.14.0",
    "daisyui": "^3.2.1",
    "eslint": "^8.42.0",
    "husky": "^8.0.0",
    "jsdom": "^22.1.0",
    "msw": "^1.2.1",
    "prettier": "^2.8.8",
    "prisma": "^5.3.1",
    "supabase": "^1.88.0",
    "ts-node": "^10.9.1",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.32.0",
    "vitest-mock-extended": "^1.1.3"
  }
}
